<h1 class="text-center">Search for cars</h1>
  <div class="welcome_container">
    <div class="row">
      <div class="col-md-3">
        <div class="latest_cars_content">
          <h2 class="latest-title">Latest cars added</h2>
          <% Car.last(3).each do |car| %>
            <div class="image_transform">
              <%= link_to car_path(car.id) do %>
                <%= image_tag car.car_image.url(:medium), class:"thumbnail car_images_welcome" %>
              <% end %>
            </div>
        <% end %>
      </div>
      </div>
      <div class="col-md-9">
        <%= render 'cars/search_form' %>
      </div>
    </div>
  </div>
